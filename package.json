{
  "name": "toad-stack",
  "version": "1.0.0",
  "description": "Placeholder package description",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build:dev": "tsc",
    "build:release": "tsup",
    "test": "vitest --coverage",
    "lint": "biome lint index.ts lib test biome.json",
    "lint:fix": "biome check --apply index.ts lib test biome.json",
    "prepublishOnly": "npm run build:release"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/node": "^20.11.16",
    "@vitest/coverage-v8": "^3.0.1",
    "tsup": "8.3.6",
    "typescript": "^5.3.3",
    "vitest": "^3.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "toad",
    "stack",
    "library",
    "template"
  ],
  "homepage": "https://github.com/kibertoad/toad-stack",
  "repository": {
    "type": "git",
    "url": "https://github.com/kibertoad/toad-stack.git"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ],
  "private": false
}
